<apidoc>
	<namespace name="SEIDS::LinkedLists::Singly">
		<class name="Item" version="1.0">
			<description>
				<p>
					This is a 'struct'-like class designed for use in the
					<class>LinkedList</class> class.
				</p>
				<p>
					It is not intended for public consumption.
				</p>
			</description>
			
			<see>
				<class>LinkedList</class>
			</see>
			
			<property name="data" access="public" version="1.0">
				The value of the item.
			</property>
			<property name="next" type="Item" access="public" version="1.0">
				The next item in the list.
			</property>
			
			<constructor access="public" version="1.0">
				<title>Constructs a new item</title>
				<description>
					This constructs a new item for use in the
					<class>LinkedList</class> class.
				</description>
				
				<parameter name="data">The value of the item.</parameter>
				<parameter type="Item" name="next" optional="yes">The next item in the list.</parameter>
			</constructor>
			
			<method name="__clone" access="public" version="1.0">
				<title>Clone the item</title>
				<description>Clones the item.</description>
			</method>
		</class>
	</namespace>
</apidoc>
